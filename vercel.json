{
  "version": 2,
  "name": "law-firm-template",
  "builds": [
    {
      "src": "build/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "/build/static/$1" },
    { "src": "/(.+\\.\\w+)$", "dest": "/build/$1" },
    { "src": "/(.*)", "dest": "/build/index.html" }
  ]
}